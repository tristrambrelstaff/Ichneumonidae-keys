<?xml version="1.0" encoding="UTF-8"?>
<key ref="Ichneumon-females-A1" type="singlet" document="Hilpert-1992" lang="de">
  <title>Gruppe A1</title>
  <singlet ref="1">
    <else>
      (<ref>4</ref>)
    </else>
    <if>
      Große Arten (13,2-15,5)
    </if>
  </singlet>
  <singlet ref="2">
    <else>
      (<ref>3</ref>)
    </else>
    <if>
      TiIII apikal zu 1/4 schwarz.
    </if>
    <then>
      <taxon>I. xanthorius FORSTER</taxon>
    </then>
  </singlet>
  <singlet ref="3">
    <else>
      (<ref>2</ref>)
    </else>
    <if>
      TiIII apikal nicht schwarz, nur wenig rot
    </if>
    <then>
      <taxon>I. sexcinctus GRAVENHORST</taxon>
    </then>
  </singlet>
  <singlet ref="4">
    <else>
      (<ref>1</ref>)
    </else>
    <if>
      Kleinere Art (bis 11,6). (Scutellum oft nicht ganz weiß, weißer
      Fleck nur nierenförmig.)
    </if>
    <then>
      <taxon>I. ostentator HEINRICH</taxon>
    </then>
  </singlet>
</key>
